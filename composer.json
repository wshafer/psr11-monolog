{
    "name": "wshafer/psr11-monolog",
    "type": "library",
    "description": "Monolog Facotories for PSR-11",
    "homepage": "https://github.com/wshafer/psr11-monolog",
    "authors": [
        {
            "name": "Westin Shafer",
            "email": "shafer_w2002@yahoo.com",
            "homepage": "https://github.com/wshafer"
        }
    ],
    "license" : "BSD-2-Clause",
    "require": {
        "php": "^7.2|^8.0",
        "psr/container": "^1.0.0",
        "monolog/monolog": "^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.0",
        "squizlabs/php_codesniffer": "^3.5.3",
        "phpmd/phpmd" : "@stable",
        "satooshi/php-coveralls": "^2.2.0",
        "swiftmailer/swiftmailer": "^6.0",
        "php-amqplib/php-amqplib": "2.6.*",
        "graylog2/gelf-php": "^1.5",
        "raven/raven": "^0.15.0",
        "php-console/php-console": "^3.1",
        "predis/predis": "^1.1",
        "mongodb/mongodb": "^1.1",
        "doctrine/couchdb": "1.0.0-beta4",
        "aws/aws-sdk-php": "^3.31",
        "ruflin/elastica": "^5.2",
        "symfony/dependency-injection": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "WShafer\\PSR11MonoLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WShafer\\PSR11MonoLog\\Test\\": "tests/"
        }
    },

    "extra": {
        "zf": {
            "config-provider": "WShafer\\PSR11MonoLog\\ConfigProvider",
            "module": "WShafer\\PSR11MonoLog"
        }
    }
}
